
tukey_list <- list()
for (group in c("Fungi", "Bacteria")){
	tukey_group <- list()
	x = "rank_only"
	y = "RSQ.1"
	df <- hindcast_taxon[which(hindcast_taxon$metric == "RSQ.1"),]
	new.df <- cbind.data.frame(x = df$pretty_name, y = df$value)
	tukey_group = tukey(new.df$x, new.df$y)
	tukey_group$group = group
	tukey_list[[group]] <- tukey_group
}
tukey_fcast_type = do.call(rbind,tukey_list)